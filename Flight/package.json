{
  "name": "flight-service",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "test": "cross-env NODE_ENV=test node_modules/nyc/bin/nyc.js node_modules/mocha/bin/_mocha --timeout 30000 --exit --require babel-register \"./src/**/*.test.js*\"",
    "test:only": "cross-env NODE_ENV=test node_modules/nyc/bin/nyc.js node_modules/mocha/bin/_mocha --timeout 30000 --exit --require babel-register $1",
    "test:report": "nyc report",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start:dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js --exec babel-node --presets es2015,stage-2",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "build": "npm run test && npm run clean && cross-env NODE_ENV=production ./node_modules/babel-cli/bin/babel.js src --out-dir ./dist && cp -R src/email_templates dist/email_templates",
    "clean": "rimraf dist",
    "seed": "node --require babel-register ./migrations/index.seed.js"
  },
  "pre-commit": [],
  "license": "MIT",
